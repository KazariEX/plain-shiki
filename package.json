{
  "name": "plain-shiki",
  "type": "module",
  "version": "0.3.1",
  "packageManager": "pnpm@10.16.1",
  "description": "Highlight your plaintext via Shiki",
  "author": "KazariEX",
  "license": "MIT",
  "repository": "KazariEX/plain-shiki",
  "keywords": [
    "shiki",
    "highlight",
    "plaintext"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:playground": "pnpm -C playground run generate",
    "dev": "tsdown -w",
    "prepack": "pnpm run build",
    "release": "bumpp --no-push -c \"release: v%s\"",
    "test": "vitest"
  },
  "peerDependencies": {
    "shiki": ">=1.22.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@zinkawaii/eslint-config": "catalog:",
    "@zinkawaii/stylelint-config": "catalog:",
    "@zinkawaii/tsconfig": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "shiki": "catalog:",
    "stylelint": "catalog:",
    "tsdown": "catalog:",
    "vitest": "catalog:"
  }
}
