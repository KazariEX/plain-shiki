{
  "name": "plain-shiki",
  "version": "0.0.4",
  "description": "Highlight your plaintext via Shiki",
  "keywords": [
    "shiki",
    "highlight",
    "plaintext"
  ],
  "author": "KazariEX",
  "repository": "KazariEX/plain-shiki",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "build:playground": "pnpm -C playground run build",
    "dev": "tsup-node --watch"
  },
  "dependencies": {
    "shiki": "^1.18.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.1",
    "@types/node": "^22.5.5",
    "@zinkawaii/eslint-config": "^0.1.7",
    "eslint": "^9.11.0",
    "tsup": "^8.3.0"
  }
}
