{
  "name": "plain-shiki",
  "type": "module",
  "version": "0.0.12",
  "packageManager": "pnpm@10.2.0",
  "description": "Highlight your plaintext via Shiki",
  "author": "KazariEX",
  "license": "MIT",
  "repository": "KazariEX/plain-shiki",
  "keywords": [
    "shiki",
    "highlight",
    "plaintext"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "build:playground": "pnpm -C playground run generate",
    "dev": "tsup-node --watch",
    "prepack": "pnpm run build",
    "release": "bumpp --no-push -c \"release: v%s\"",
    "test": "vitest",
    "eslint": "eslint ."
  },
  "peerDependencies": {
    "shiki": "^1.22.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@types/node": "^22.13.1",
    "@zinkawaii/eslint-config": "^0.1.10",
    "@zinkawaii/stylelint-config": "^0.2.2",
    "bumpp": "^10.0.1",
    "eslint": "^9.19.0",
    "shiki": "^2.3.1",
    "stylelint": "^16.14.1",
    "tsup": "^8.3.6",
    "vitest": "^3.0.5"
  }
}